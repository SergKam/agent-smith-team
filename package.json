{
  "name": "agent-smith-team",
  "version": "1.0.0",
  "description": "AI agent development team",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "tsc && node src/server.js",
    "dev": "ts-node src/apps/taskManager/server.ts",
    "generate-types": "openapi-typescript src/shared/api.yaml --output src/apps/taskManager/types/api-types.ts",
    "mysql": "docker run --rm -e MYSQL_ROOT_PASSWORD=localsecret  --mount type=bind,src=$(pwd)/data,dst=/var/lib/mysql  -p 3306:3306 mysql:5",
    "ask": "CI=true ts-node src/apps/agentDeveloper/askgpt.ts",
    "test": "tsc && jest --no-color",
    "tsc": "tsc",
    "format": "prettier --write .",
    "clean": "find . -type f -name '*.js' ! -path './node_modules/*' -delete",
    "ui:build": "remix vite:build",
    "ui:dev": "remix vite:dev",
    "ui:lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "ui:start": "remix-serve ./build/server/index.js"
  },
  "keywords": [],
  "author": "Sergii Kamenskyi",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.35",
    "@ai-sdk/openai": "^0.0.40",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "ai": "^3.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-openapi": "^12.1.3",
    "glob": "^10.4.5",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.10.1",
    "openapi-typescript": "^7.0.0",
    "winston": "^3.13.1",
    "@remix-run/node": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@remix-run/serve": "^2.11.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.10.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "@remix-run/dev": "^2.11.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
